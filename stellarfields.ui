<?xml version='1.0' encoding='utf-8'?>
<interface version="1.2">
  <object class="tk.Toplevel" id="main_window">
    <property name="geometry">400x640</property>
    <property name="padx">10</property>
    <property name="pady">10</property>
    <property name="title" translatable="yes">stellarfields</property>
    <child>
      <object class="ttk.Frame" id="frame1">
        <layout manager="pack">
          <property name="expand">true</property>
          <property name="fill">both</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="ttk.Scrollbar" id="events_scroll">
            <property name="orient">vertical</property>
            <layout manager="pack">
              <property name="fill">y</property>
              <property name="side">right</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Treeview" id="events_table">
            <property name="selectmode">extended</property>
            <property name="show">headings</property>
            <property name="yscrollcommand" type="command" cbtype="scrollset">set</property>
            <layout manager="pack">
              <property name="anchor">nw</property>
              <property name="expand">true</property>
              <property name="fill">both</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
